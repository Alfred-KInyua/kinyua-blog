<h2>Listing all articles</h2>
<%=link_to 'create new article', new_article_path%>
<table>
<tr>
<th>Title</th><th>Description</th>
<tr>
<tbody>
<%@displayAll.each do|f|%>
<tr>
<td><%=f.title%></td>
<td><%=f.description%></td>
<td><%=link_to 'Edit', edit_article_path(f)%></td>
<td><%=link_to 'Show', article_path(f)%></td>
</tr>
<%end%>
<tbody>
</table>